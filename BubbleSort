
def bubblesort(x):
    n=len(x)
    for j in range(n,1,-1):
        for i in range(0,j-1):
            if x[i]<x[i+1]: 
                pass
            else: 
                x[i]=x[i]+x[i+1]
                x[i+1]=x[i]-x[i+1]
                x[i]=x[i]-x[i+1]
    return x
x=[5,4,-3,2,1]
print(bubblesort(x))
    
